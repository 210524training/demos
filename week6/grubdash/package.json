{
  "name": "grubdash",
  "version": "1.0.0",
  "description": "A DoorDash/GrubHub Clone",
  "main": "src/index.ts",
  "scripts": {
    "test": "jest",
    "test:cov": "jest --coverage",
    "start": "nodemon --exec ts-node src/index.ts",
    "start:prod": "npm run build && node grubdash.js",
    "lint": "eslint **",
    "lint:fix": "eslint ** --fix",
    "tsc": "tsc",
    "build": "esbuild src/index.ts --bundle --minify --platform=node --sourcemap --outfile=grubdash.js",
    "setup": "ts-node src/setup/createTable.ts",
    "teardown": "ts-node src/setup/deleteTable.ts",
    "populate": "ts-node src/setup/populateData.ts"
  },
  "author": "IkenoXamos",
  "license": "ISC",
  "devDependencies": {
    "@types/cors": "^2.8.10",
    "@types/express": "^4.17.12",
    "@types/express-session": "^1.17.3",
    "@types/fs-extra": "^9.0.11",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.6.1",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "esbuild": "^0.12.15",
    "eslint": "^7.27.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-import": "^2.23.3",
    "fs-extra": "^10.0.0",
    "jest": "^27.0.1",
    "nodemon": "^2.0.7",
    "randexp": "^0.5.3",
    "ts-jest": "^27.0.1",
    "ts-node": "^10.0.0",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "aws-sdk": "^2.920.0",
    "cors": "^2.8.5",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "express-async-errors": "^3.1.1",
    "express-session": "^1.17.2",
    "http-status-codes": "^2.1.4",
    "log4js": "^6.3.0",
    "uuid": "^8.3.2"
  }
